{
    "name": "@ultimaker/ultimaker.com-designsystem",
    "version": "0.0.8",
    "description": "Ultimaker.com design system for vue components",
    "repository": "github:Ultimaker/Ultimaker.com-designsystem",
    "license": "UNLICENSED",
    "module": "dist/components.js",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "files": [
        "src",
        "dist"
    ],
    "scripts": {
        "clean": "rimraf dist/**",
        "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.amd.config.js --progress --hide-modules",
        "build:dev": "cross-env NODE_ENV=development webpack --config webpack.amd.config.js --progress --hide-modules",
        "build-storybook": "cross-env NODE_ENV=production build-storybook -s public",
        "storybook": "start-storybook -p 9090 -s public",
        "test": "npm run unit",
        "unit": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
        "unit:chrome": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run --chrome",
        "unit:debug": "cross-env BABEL_ENV=test karma start test/karma.conf.js --debug",
        "snapshots": "cross-env BABEL_ENV=jest jest --silent --updateSnapshot --config=jest.config.js",
        "snapshots:update": "cross-env BABEL_ENV=jest jest --silent -u --config=jest.config.js",
        "lint": "eslint './src/js/**/*.js'",
        "lint:debug": "eslint './src/js/**/*.js' --debug"
    },
    "dependencies": {
        "core-js": "^2.5.7",
        "gsap": "^2.0.2",
        "vue": "^2.5.17",
        "vuex": "^3.0.1",
        "whatwg-fetch": "^2.0.4",
        "express": "^4.16.3"
    },
    "devDependencies": {
        "@storybook/addon-actions": "4.0.0-alpha.14",
        "@storybook/addon-backgrounds": "4.0.0-alpha.14",
        "@storybook/addon-centered": "4.0.0-alpha.14",
        "@storybook/addon-knobs": "4.0.0-alpha.14",
        "@storybook/addon-links": "4.0.0-alpha.14",
        "@storybook/addon-notes": "4.0.0-alpha.14",
        "@storybook/addon-options": "4.0.0-alpha.14",
        "@storybook/addon-storyshots": "4.0.0-alpha.14",
        "@storybook/addon-storysource": "4.0.0-alpha.14",
        "@storybook/addon-viewport": "4.0.0-alpha.14",
        "@storybook/addons": "4.0.0-alpha.14",
        "@storybook/vue": "4.0.0-alpha.14",
        "babel-core": "^6.26.3",
        "babel-eslint": "8.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^7.1.4",
        "babel-plugin-istanbul": "^4.1.6",
        "babel-plugin-require-context-hook": "^1.0.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-2": "^6.24.1",
        "babel-preset-vue": "^2.0.2",
        "cross-env": "^5.1.6",
        "eslint": "^4.19.1",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-promise": "3.8.0",
        "eslint-plugin-vue": "^4.5.0",
        "file-loader": "^1.1.11",
        "html-loader": "^0.5.5",
        "html-loader-jest": "^0.2.1",
        "intl": "^1.2.5",
        "jasmine": "^2.8.0",
